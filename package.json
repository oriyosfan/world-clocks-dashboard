{
  "name": "world-clocks-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "vitest run --reporter=verbose | cat",
    "check": "npm run lint && npm run typecheck",
    "predev": "npm i && npm run check",
    "dev": "concurrently -k -n NEXT,LINT \"next dev\" \"npm run lint:watch\"",
    "build": "next build",
    "prelint": "node -e \"require('fs').mkdirSync('.next/cache/eslint',{recursive:true})\"",
    "lint": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --max-warnings 0 --cache --cache-location .next/cache/eslint/.eslintcache",
    "lint:watch": "nodemon --ext ts,tsx,js,mjs,cjs --watch src --watch app --watch pages --watch next.config.ts --watch postcss.config.mjs --ignore node_modules --ignore .next --exec \"npm run lint\"",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --fix --cache --cache-location .next/cache/eslint/.eslintcache",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "cypress open --config-file cypress.config.js | cat",
    "cy:run": "cypress run --config-file cypress.config.js | cat"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^5.26.7",
    "countries-and-timezones": "^3.8.0",
    "country-flag-icons": "^1.5.19",
    "eslint-webpack-plugin": "^5.0.2",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^25.0.1",
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/nextjs-vite": "^9.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "cypress": "^13.13.2",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^9.1.1",
    "tailwindcss": "^4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  }
}
