{
  "name": "world-clocks-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "npm run lint && npm run typecheck",
    "predev": "npm i && npm run check",
    "dev": "concurrently -k -n NEXT,LINT \"next dev\" \"npm run lint:watch\"",
    "prebuild": "npm ci && npm run check",
    "build": "next build",
    "prelint": "node -e \"require('fs').mkdirSync('.next/cache/eslint',{recursive:true})\"",
    "lint": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --max-warnings 0 --cache --cache-location .next/cache/eslint/.eslintcache",
    "lint:watch": "nodemon --ext ts,tsx,js,mjs,cjs --watch src --watch app --watch pages --watch next.config.ts --watch postcss.config.mjs --ignore node_modules --ignore .next --exec \"npm run lint\"",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --fix --cache --cache-location .next/cache/eslint/.eslintcache",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "next start"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.1.0",
    "antd": "^5.26.7",
    "eslint-webpack-plugin": "^5.0.2",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
